<!DOCTYPE html>
<html>
<head>
    <% include ../partials/meta %>
    <title><%= title %></title>
    <meta name="keywords" content="" >
    <meta name="description" content="" >
    <% include ../partials/css %>
    <% include ../partials/tongji %>
</head>

<body class="index-page">

<% include ../partials/header-sub %>


<div class="main-tab am-g am-g-fixed">
    <div class="am-u-sm-4">
        <div class="am-dropdown" data-am-dropdown>
            <button class="am-btn am-btn-primary am-dropdown-toggle" data-am-dropdown-toggle>下拉列表 <span class="am-icon-caret-down"></span></button>
            <ul class="am-dropdown-content">
                <li class="am-dropdown-header">标题</li>
            </ul>
        </div>
    </div>
    <div class="am-u-sm-4">
        <div class="am-dropdown" data-am-dropdown>
            <button class="am-btn am-btn-primary am-dropdown-toggle" data-am-dropdown-toggle>下拉列表 <span class="am-icon-caret-down"></span></button>
            <ul class="am-dropdown-content">
                <li class="am-dropdown-header">标题</li>
            </ul>
        </div>
    </div>
    <div class="am-u-sm-4">
        <div class="am-dropdown" data-am-dropdown>
            <button class="am-btn am-btn-primary am-dropdown-toggle" data-am-dropdown-toggle>下拉列表 <span class="am-icon-caret-down"></span></button>
            <ul class="am-dropdown-content">
                <li class="am-dropdown-header">标题</li>
            </ul>
        </div>
    </div>
</div>

<form action="/search" method="get">
    <input type="text" value="<%= keywords %>" name="keywords" /><input type="submit" value="搜索" />
</form>
<a href="/search?keywords=<%= keywords %>&order=sales">销量</a>
<a href="/search?keywords=<%= keywords %>&order=price">价格</a>
<a href="/search?keywords=<%= keywords %>&order=pageViews">人气</a>
<div>
<input type="checkbox" name="inventory" value="1" <% if(inventory) { %> checked <% } %> />现货
<input type="checkbox" name="isHandiwork" value="1"  <% if(isHandiwork) { %> checked <% } %> />手工
</div>
<% if (count > 0) { %>
<table>
    <% for(var i=0; i < items.length; i++) {%>
    <tr>
        <td><a href="/detail?id=<%=items[i].attributes.productAllId %>"><%= items[i].attributes.name %></a></td>
        <td><img width="132" src="<%= items[i].attributes.coverImage %>" /></td>
        <td><%= items[i].attributes.salePrice %></td>
    </tr>
    <% } %>
</table>
<% } %>


<% include ../partials/footer %>
<% include ../partials/js %>
<script>
    $(function() {
        
    });
</script>
</body>
</html>