<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"/>
    <meta name="renderer" content="webkit">
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <link rel="icon" type="image/png" href="/assets/i/favicon.png">
    <link rel="apple-touch-icon-precomposed" href="/assets/i/app-icon72x72@2x.png">
    <meta name="apple-mobile-web-app-title" content="51wish.cn" />
    <title><%= title %></title>
    <% include ../partials/css %>
</head>
<body class="book-oils">

    <div class="container">
        <h1><%= category2.get('name')%></h1>
        <table>
            <% for(var i=0; i< product.length;i++) {%>
            <tr>
                <td class="img">
                    <div class="img-border">
                        <img src="<% for(var n in product[i].get('mainImage')) { %><% if(product[i].attributes.mainImage[n].isMainImage) { %><%= product[i].attributes.mainImage[n].url.replace('ac-QuiPuWpJ.clouddn.com','lc-QuiPuWpJ.cn-n1.lcfile.com').replace(/lc-QuiPuWpJ.cn-n1.lcfile.com/gi, 'image.wish698.cn') %>?imageMogr2/thumbnail/120<%break;}}%>" alt="">
                    </div>
                </td>
                <td class="text">
                    <h3><%= product[i].get('name') %></h3>
                    <h4><%= product[i].get('detail') %></h4>
                </td>
            </tr>
            <% } %>
        </table>
    </div>

    <% include ../partials/js %>
    <script type="text/javascript">
        $(function () {
            var bookOils = require('book-oils');
            bookOils.indexFun();
        });
    </script>
</body>
</html>