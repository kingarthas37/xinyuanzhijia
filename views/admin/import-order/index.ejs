<script type="text/javascript">
    $(function () {
        var inputWisdomOrder = $('.input-wisdom-order');
        var inputWisdomValue = $.trim(inputWisdomOrder.val());
        $('.btn-import-wisdom-order').click(function () {
            var arrWisdomData = [];
            var arrValue = inputWisdomValue.split('\n');
            $.each(arrValue,function (i,n) {
                n = n.replace(/\t/g,' ');
                n = n.replace(/\s+/g,' ');
                arrWisdomData.push({
                    name:$.trim(/([^\$]+)\$/.exec(n)[1]),
                    count:/\$[^\s]+\s(\d+)/.exec(n)[1]
                });
            });
            console.log(arrWisdomData);
            $.ajax({
                url:'/admin/import-order/data',
                type:'post',
                data:{'import-data': JSON.stringify(arrWisdomData)}
            })
        });


        var inputLuckymojoOrder = $('.input-luckymojo-order');
        var inputLuckymojoValue = $.trim(inputLuckymojoOrder.val());
        $('.btn-import-luckymojo-order').click(function () {
            var arrLuckymojoData = [];
            var arrValue = inputLuckymojoValue.split('\n');
            $.each(arrValue,function (i,n) {
                n = n.replace(/\t/g,' ');
                n = n.replace(/\s+/g,' ');
                arrLuckymojoData.push({
                    name:/(\w+\-\w+\-\w+)/.exec(n)[1],
                    count:/(\d+)/.exec(n)[1]
                });
            });
            console.log(arrLuckymojoData);
            $.ajax({
                url:'/admin/import-order/data',
                data:JSON.stringify(arrLuckymojoData)
            })
        });


    });
</script>
