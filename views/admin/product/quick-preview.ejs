<!DOCTYPE html>
<html>
<head>
    <% include ../partials/meta %>
    <title><%= title %></title>
    <% include ../partials/css %>
</head>
<body>

    <% include ../partials/header %>

    <div class="product-preview-page">

        <h4><%= name %></h4>

        <hr/>
        
        <div class="preview-content">

                <%if(banner) { %><section><img src="<%=banner%>"/></section><%}%>

                <% if (name) { %>
                <section><img src="https://img.alicdn.com/imgextra/i3/42879206/TB2ehN0hFXXXXaIXpXXXXXXXXXX_!!42879206.png"></section>
                <div><%- name %></div>
                <% } %>

                <% if (review) { %>
                <section><img src="https://img.alicdn.com/imgextra/i3/42879206/TB2XdJYhFXXXXbhXpXXXXXXXXXX_!!42879206.png"></section>
                <div><%- review %></div>
                <% } %>

                <% if (property) { %>
                <section><img src="https://img.alicdn.com/imgextra/i3/42879206/TB23rh5hFXXXXXBXpXXXXXXXXXX_!!42879206.png"></section>
                <div><%- property %></div>
                <% } %>

                <% if (instruction) { %>
                <section><img src="https://img.alicdn.com/imgextra/i3/42879206/TB2w3VZhFXXXXaCXpXXXXXXXXXX_!!42879206.png"/></section>
                <div class="content"><%- instruction %></div>
                <% } %>

                <% if (detail) { %>
                <section><img src="https://img.alicdn.com/imgextra/i3/42879206/TB2LWd4hFXXXXXqXpXXXXXXXXXX_!!42879206.png"/></section>
                <div class="content"><%- detail %></div>
                <% } %>

                <% if (use) { %>
                <section><img src="https://img.alicdn.com/imgextra/i1/42879206/TB2bUxTpVXXXXanXpXXXXXXXXXX_!!42879206.png"/></section>
                <div class="content"><%- use %></div>
                <% } %>

                <% if (detailImage) { %>
                <section><img src="https://img.alicdn.com/imgextra/i3/42879206/TB2DOWrhFXXXXXiXXXXXXXXXXXX_!!42879206.png"/></section>
                <article><%- detailImage %></article>
                <% } %>
        </div>

    </div>
    
    <% include ../partials/footer %>
    <% include ../partials/js %>

</body>
</html>